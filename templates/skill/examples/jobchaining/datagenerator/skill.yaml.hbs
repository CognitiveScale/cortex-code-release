camel: 1.0.0
name: {{dashCase skillname}}-datagenerator
title: Data generator job skill
description: This skill demonstrates the use of a job skill
inputs:
  - name: input
    title: input
    routing:
      all:
        action: default-action
        output: output
        runtime: cortex/jobs
outputs:
  - name: output
    title: output
actions:
  - name: default-action
    description: {{skillname}} Action
    image: {{repo.url}}/{{dashCase skillname}}-action
    command: []
    port: "8080"
    type: job
    scaleCount: 1
    environmentVariables: ""
